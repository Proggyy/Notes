<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Orange">#F5923D</Color>
    <SolidColorBrush x:Key="OrangeBrush" Color="{StaticResource Orange}"/>
    <Color x:Key="LightOrange">#F5C149</Color>
    <SolidColorBrush x:Key="LightOrangeBrush" Color="{StaticResource LightOrange}"/>
    <Color x:Key="Yellow">#F5E556</Color>
    <SolidColorBrush x:Key="YellowBrush" Color="{StaticResource Yellow}"/>
    <Color x:Key="YellowOrange">#FFD977</Color>
    <SolidColorBrush x:Key="YellowOrangeBrush" Color="{StaticResource YellowOrange}"/>
    <Color x:Key="Purple">#5D31F5</Color>
    <SolidColorBrush x:Key="PurpleBrush" Color="{StaticResource Purple}"/>
    <Color x:Key="LightPurple">#A388FF</Color>
    <SolidColorBrush x:Key="LightPurpleBrush" Color="{StaticResource LightPurple}"/>
    <Color x:Key="Red">#FF1A1A</Color>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource Red}"/>
    <Color x:Key="LightRed">#FF7676</Color>
    <SolidColorBrush x:Key="LightRedBrush" Color="{StaticResource LightRed}"/>
</ResourceDictionary>